            @Html.DisplayFor(modelItem => item.ban)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.yearOfStudy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Campu.campusName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PayType1.payType1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserType1.userType1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.userID }) |
            @Html.ActionLink("Details", "Details", new { id=item.userID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.userID })
        </td>
    </tr>
}

</table>
